# Airflow Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env files to version control

# Airflow Core Configuration
AIRFLOW_UID=50000
AIRFLOW_PROJ_DIR=.

# AWS Configuration
AWS_REGION=ap-southeast-2
AWS_DEFAULT_REGION=ap-southeast-2

# Database Configuration for Airflow Variables and Connections
REDSHIFT_ADMIN_USERNAME=admin
REDSHIFT_ADMIN_PASSWORD="your_secure_password_here"
REDSHIFT_DATABASE_NAME=divvy
REDSHIFT_WORKGROUP=divvybikes-dev
REDSHIFT_NAMESPACE=divvybikes-dev
REDSHIFT_ENDPOINT=your_redshift_endpoint_here
REDSHIFT_PORT=5439

# S3 Bucket Names
BRONZE_BUCKET=divvybikes-dev-bronze-data-youraccountid
SILVER_BUCKET=divvybikes-dev-silver-data-youraccountid
GOLD_BUCKET=divvybikes-dev-gold-data-youraccountid

# Airflow Database Configuration
POSTGRES_USER=airflow
POSTGRES_PASSWORD=airflow
POSTGRES_DB=airflow
